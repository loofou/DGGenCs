  "version": "0.5.0",
  "configurations": [
    {
      "name": "10 Agents",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0/DGGenCs.dll",
      "args": ["--verbose","-c","10","-v", "-d"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
    {
      "name": "10 NPCs",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0/DGGenCs.dll",
      "args": ["--verbose","-c","10", "-t","npc", "-a", "20"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    }
  ]
}